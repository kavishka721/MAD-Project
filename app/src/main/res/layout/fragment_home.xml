<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".bottomNavbar.HomeFragment">

    <!-- TODO: Update blank fragment layout -->
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#DA545555"
        android:backgroundTintMode="screen"
        >

        <TextView
            android:id="@+id/textViewHome1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/textViewHome1"
            android:textColor="@color/textColor"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/partsname_casings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/casing"
            android:textColor="@color/textColor"
            app:drawableTint="#FFFFFF"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button3" />

        <TextView
            android:id="@+id/partname_processor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_processor"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="@+id/button4"
            app:layout_constraintTop_toBottomOf="@+id/button4" />

        <TextView
            android:id="@+id/partname_moterboard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_moterboard"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="@+id/button9"
            app:layout_constraintTop_toBottomOf="@+id/button9" />

        <TextView
            android:id="@+id/partname_powerSupply"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_powersupply"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button10" />

        <TextView
            android:id="@+id/partname_ram"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_memory"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="@+id/button11"
            app:layout_constraintTop_toBottomOf="@+id/button11" />

        <TextView
            android:id="@+id/partname_harddisk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_harddisk"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="@+id/button13"
            app:layout_constraintTop_toBottomOf="@+id/button13" />

        <TextView
            android:id="@+id/partname_ssd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_ssd"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button14" />

        <TextView
            android:id="@+id/partname_graphiccard"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_graphiccard"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="@+id/button15"
            app:layout_constraintTop_toBottomOf="@+id/button15" />

        <TextView
            android:id="@+id/partname_opticaldrive"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_opticaldrive"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="@+id/button16"
            app:layout_constraintTop_toBottomOf="@+id/button16" />

        <TextView
            android:id="@+id/partname_cooling"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_cooling_and_lighting"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="@+id/button18"
            app:layout_constraintTop_toBottomOf="@+id/button18" />

        <TextView
            android:id="@+id/partname_monitor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/doppio_one"
            android:text="@string/partname_monitor"
            android:textColor="@color/textColor"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button17" />

        <Button
            android:id="@+id/button3"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:hint="@string/casing"
            android:background="@drawable/cooler_master_mastercase_h500p_mesh_argb"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewHome1" />

        <Button
            android:id="@+id/button4"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/processor"
            app:layout_constraintStart_toEndOf="@+id/button3"
            app:layout_constraintTop_toTopOf="@+id/button3"/>



        <Button
            android:id="@+id/button9"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/motherboard1"
            app:layout_constraintStart_toEndOf="@+id/button4"
            app:layout_constraintTop_toTopOf="@+id/button4"

            />

        <Button
            android:id="@+id/button10"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/powersupply1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/partsname_casings"
            />

        <Button
            android:id="@+id/button11"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/ramcard1"
            app:layout_constraintStart_toEndOf="@+id/button10"
            app:layout_constraintTop_toTopOf="@+id/button10"
            />

        <Button
            android:id="@+id/button13"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/harddisk"
            app:layout_constraintStart_toEndOf="@+id/button11"
            app:layout_constraintTop_toTopOf="@+id/button11" />

        <Button
            android:id="@+id/button14"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/ssd2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/partname_powerSupply"

            />

        <Button
            android:id="@+id/button15"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/graphiccard2"
            app:layout_constraintStart_toEndOf="@+id/button14"
            app:layout_constraintTop_toTopOf="@+id/button14"
            />

        <Button
            android:id="@+id/button16"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/opticaldrive"
            app:layout_constraintStart_toEndOf="@+id/button15"
            app:layout_constraintTop_toTopOf="@+id/button15"/>


        <Button
            android:id="@+id/button17"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/monitor3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/partname_ssd"/>


        <Button
            android:id="@+id/button18"
            android:layout_width="115dp"
            android:layout_height="110dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/coolingsystem3"
            app:layout_constraintStart_toEndOf="@+id/button17"
            app:layout_constraintTop_toTopOf="@+id/button17"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>