<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/hback"
    tools:context=".PCConf" >

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="417dp"
        android:layout_height="567dp"
        android:layout_marginTop="136dp"
        android:background="@drawable/intersectionew3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="436dp"
        android:layout_height="648dp"
        android:layout_marginTop="112dp"
        android:layout_marginEnd="4dp"
        android:background="@drawable/intersectionew2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="368dp"
        android:layout_height="636dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="136dp"
        android:background="@drawable/intersectionew1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button13"
        android:layout_width="110dp"
        android:layout_height="107dp"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="96dp"
        android:background="@drawable/option3"
        android:fontFamily="@font/doppio_one"
        android:text="@string/create_pcbtn"
        android:textColor="#AFAFAF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button20"
        android:layout_width="110dp"
        android:layout_height="107dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="96dp"
        android:background="@drawable/option3"
        android:fontFamily="@font/doppio_one"
        android:text="@string/saved_confbtn"
        android:textColor="#AFAFAF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="166dp"
        android:layout_height="178dp"
        android:layout_marginTop="200dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/pc" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:fontFamily="@font/doppio_one"
        android:text="@string/pc_configuration_station"
        android:textAlignment="center"
        android:textColor="#DADADA"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>